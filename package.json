{
  "name": "@xeroui/vue",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/xui-vue.umd.cjs",
  "module": "./dist/xui-vue.js",
  "exports": {
    ".": {
      "import": "./dist/xui-vue.js",
      "require": "./dist/xui-vue.umd.cjs"
    },
    "./style.css": "./dist/xui-vue.css"
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview",
    "fmt": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.541.0",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.19"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/test": "^9.0.0-alpha.2",
    "@storybook/vue3": "^9.1.3",
    "@storybook/vue3-vite": "^9.1.3",
    "@tailwindcss/vite": "^4.1.12",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vue-tsc": "^3.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/xeroui/xui-vue.git"
  },
  "author": "Arafat Islam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xeroui/xui-vue/issues"
  },
  "homepage": "https://github.com/xeroui/xui-vue#readme",
  "keywords": [
    "vue",
    "tailwindcss",
    "vite",
    "ui",
    "library"
  ],
  "description": "Vue 3 UI library with TailwindCSS v4"
}
